{
    "name": "toggle-all",
    "version": "1.2.0",
    "description": "Javascript plugin toggle dropdown, accordion, tabs",
    "main": "lib/cjs/index.js",
    "module": "lib/es/index.js",
    "types": "lib/es/index.d.ts",
    "sideEffects": false,
    "files": [
        "lib/"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build:sass": "node-sass -r --output-style compressed src/scss -d -o src",
        "build:cjs": "tsc -p ./tsconfig.cjs.json",
        "build:es": "tsc -p ./tsconfig.es.json",
        "build": "yarn run build:sass && yarn run build:es && yarn run build:cjs",
        "start": "tsc -p ./tsconfig.es.json -w",
        "dev": "rollup -c -w",
        "lint": "eslint 'src/**/*.js'",
        "lint:fix": "node node_modules/prettier-eslint 'src/**/*.js' --write",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
        "test:staged": "jest --findRelatedTests",
        "css": "node-sass src/scss -d -o dist",
        "css:watch": "yarn run css && node-sass src/scss -d -wo dist",
        "server": "browser-sync --port 6477 start --server 'dist' --files 'dist'",
        "watch:server": "concurrently --kill-others \"yarn run dev\" \"yarn run css:watch\" \"yarn run server\"",
        "storybook": "start-storybook -p 9001 -c storybook",
        "storybook:build": "build-storybook  -c storybook",
        "prepublish": "npm run build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    },
    "browserslist": [
        "Chrome >= 72",
        "ChromeAndroid >= 72",
        "Firefox >= 65",
        "iOS >=12",
        "IE >= 11"
    ],
    "engines": {
        "node": ">=8"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "yarn run lint:fix",
            "yarn run test:staged",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/NaciAkce/toggle-all.git"
    },
    "keywords": [
        "toggle",
        "accordion",
        "tabs",
        "menu"
    ],
    "author": "Naci Akce",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NaciAkce/toggle-all/issues"
    },
    "homepage": "https://naciakce.github.io/toggle-all",
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@storybook/addon-actions": "^6.3.8",
        "@storybook/addon-essentials": "^6.3.8",
        "@storybook/addon-links": "^6.3.8",
        "@storybook/addon-options": "^5.3.13",
        "@storybook/addons": "^6.3.8",
        "@storybook/html": "^6.3.8",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "autoprefixer": "^10.3.4",
        "babel-jest": "^27.2.0",
        "babel-loader": "^8.0.6",
        "browser-sync": "^2.26.7",
        "concurrently": "^6.2.1",
        "coveralls": "^3.0.9",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.20.1",
        "gh-pages": "^3.2.3",
        "husky": "^7.0.2",
        "jest": "^27.2.0",
        "lint-staged": "^11.1.2",
        "node-sass": "^6.0.1",
        "postcss": "^8.2.10",
        "resolve-url-loader": "^4.0.0",
        "rollup": "^2.56.3",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-gzip": "^2.3.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-watch": "^4.3.1",
        "sass-loader": "^12.1.0",
        "uuid": "^8.3.2"
    },
    "dependencies": {}
}
