{
    "name": "toggle-all",
    "version": "1.0.0",
    "description": "Javascript plugin toggle dropdown, accordion, tabs",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf dist",
        "prebuild": "yarn run clean",
        "build": "rollup -c",
        "lint": "eslint '**/*.js'",
        "lint:fix": "node node_modules/prettier-eslint '**/*.js' --write"
    },
    "browserslist": [
        "> 0.25%, ie 11, not op_mini all, not dead"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "yarn run lint:fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NaciAkce/toggle-all.git"
    },
    "keywords": [
        "toggle",
        "accordion",
        "tabs",
        "menu"
    ],
    "author": "Naci Akce",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NaciAkce/toggle-all/issues"
    },
    "homepage": "https://github.com/NaciAkce/toggle-all#readme",
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-react": "^7.0.0",
        "eslint": "^5.15.3",
        "eslint-config-prettier": "^4.1.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-react": "^7.12.4",
        "eslint-plugin-standard": "^4.0.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.5",
        "prettier-eslint": "^8.8.2",
        "rollup": "^1.7.0",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-node-resolve": "^4.0.1",
        "rollup-plugin-terser": "^4.0.4"
    }
}
