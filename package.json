{
  "name": "toggle-all",
  "version": "1.2.0",
  "description": "Javascript plugin toggle dropdown, accordion, tabs",
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "types": "lib/es/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:sass": "node-sass -r --output-style expanded src/scss -d -o src",
    "build:css": "node-sass --output-style expanded src/scss -d -o dist && purgecss --css dist/*.css --content dist/index.html --output dist    ",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:es": "tsc -p ./tsconfig.es.json",
    "build": "yarn run build:sass && yarn run build:es && yarn run build:cjs",
    "start:server": "browser-sync --port 6477 start --server 'dist' --files 'dist'",
    "start:css": "yarn run build:css && node-sass src/scss -d -wo dist",
    "start:js": "tsc -p ./tsconfig.es.json -w",
    "start": "concurrently --kill-others \"yarn run start:js\" \"yarn run start:css\" \"yarn run storybook\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:staged": "jest --findRelatedTests",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook  -c .storybook",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "browserslist": [
    "Chrome >= 72",
    "ChromeAndroid >= 72",
    "Firefox >= 65",
    "iOS >=12",
    "IE >= 11"
  ],
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaciAkce/toggle-all.git"
  },
  "keywords": [
    "toggle",
    "accordion",
    "tabs",
    "menu"
  ],
  "author": "Naci Akce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NaciAkce/toggle-all/issues"
  },
  "homepage": "https://naciakce.github.io/toggle-all",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/config-lerna-scopes": "^13.1.0",
    "@storybook/addon-options": "^5.3.13",
    "@storybook/addons": "^6.3.8",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/jest": "^27.0.1",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.3.4",
    "babel-jest": "^27.2.0",
    "browser-sync": "^2.26.7",
    "concurrently": "^6.2.1",
    "coveralls": "^3.0.9",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^4.9.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "purgecss": "^4.0.3",
    "resolve-url-loader": "^4.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2",
    "@babel/core": "^7.15.5",
    "@reapit/storybook-addon-html": "^5.0.1",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-viewport": "^6.3.8",
    "@storybook/builder-webpack5": "^6.3.8",
    "@storybook/html": "^6.3.8",
    "@storybook/manager-webpack5": "^6.3.8",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "storybook-code-panel": "^0.2.1",
    "style-loader": "^3.3.0"
  },
  "dependencies": {}
}
